{
  "name": "@lilith-plat/web-design-test",
  "version": "1.1.6",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build:types": "tsc -p tsconfig.build.json && cpr lib es",
    "clean": "rimraf lib es dist",
    "preview:site": "npm run docs:build && serve doc-site",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "compile-antd": "rimraf ./compiled && node ./scripts/compileAntd",
    "build": "yarn clean && father build && cpr src/antd-config.js dist/antd-config.js",
    "release": "lerna publish"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "src/**/style/*",
    "./src/**/style/**",
    "*.less"
  ],
  "maintainers": [
    {
      "name": "liang.dong"
    }
  ],
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "unpkg": "dist/antd.min.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@lilith-plat/utils": "file:../utils",
    "babel-plugin-import": "^1.13.5",
    "react": "^18.0.0"
  }
}
